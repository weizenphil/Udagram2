        "reflect-metadata": "^0.2.2",
        "sequelize-typescript": "^2.1.6"
    },
    "devDependencies": {
        "bluebird": "^3.7.2",
        "chai": "^5.1.0",
        "chai-http": "^4.4.0",
        "cors": "^2.8.5",
        "eslint": "^9.0.0",
        "eslint-plugin": "^1.0.1",
        "express": "^4.19.2",
        "mocha": "^10.4.0",
        "node": "^18.20.2",
        "parser": "^0.1.4",
        "sequelize": "^6.37.3",
        "validator": "^13.11.0"
    }
